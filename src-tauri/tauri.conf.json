{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "my-finance",
	"version": "0.1.0",
	"identifier": "com.my-finance.app",
	"build": {
		"beforeDevCommand": "npm run vite",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "npm run build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "My Finance",
				"width": 1200,
				"height": 1000,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
